<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" prefix="metadata" url="http://www.mulesoft.org/schema/mule/metadata">
  <global-cloud-connector extends="http://www.mulesoft.org/schema/mule/metadata/abstractMetadataConnectorGeneric" image="icons/large/metadata-connector-48x32.png" localId="config" icon="icons/small/metadata-connector-24x16.png" description="Global Metadata configuration information." caption="Metadata">
    <attribute-category description="Metadata configuration properties." caption="Metadata">
      <group caption="Generic" id="metadataGenericProperties">
        <name required="false" description="Give a name to this configuration so it can be later referenced by config-ref." caption="Name" name="name"></name>
      </group>
      <group caption="Group1" id="group1">
        <string required="true" description="Configurable field in group 1." caption="Configurable1" name="configurable1"></string>
        <string required="true" description="Configurable field in group 1." caption="Configurable3" name="configurable3"></string>
      </group>
      <group caption="Group2" id="group2">
        <string required="true" description="Configurable field in group 2." caption="Configurable2" name="configurable2"></string>
      </group>
    </attribute-category>
  </global-cloud-connector>
  <cloud-connector extends="http://www.mulesoft.org/schema/mule/metadata/abstractMetadataConnectorGeneric" image="icons/large/metadata-connector-48x32.png" localId="metadata-connector" icon="icons/small/metadata-connector-24x16.png" description="Module to test annotations applied to parameters." caption="Metadata" aliasId="org.mule.tooling.ui.modules.core.pattern.metadata">
    <attribute-category description="General." caption="General">
      <group caption="Generic" id="metadataConnectorGeneric">
        <modeSwitch description="Operation." caption="Operation" name="Metadata operations to execute">
          <mode modeLabel="Processor" modeId="http://www.mulesoft.org/schema/mule/metadata/processor"></mode>
        </modeSwitch>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector abstract="true" image="icons/large/metadata-connector-48x32.png" localId="abstractMetadataConnectorGeneric" icon="icons/small/metadata-connector-24x16.png" description="Interact with Metadata." caption="Abstractmetadataconnectorgeneric">
    <attribute-category description="General." caption="General">
      <group caption="Generic" id="abstractMetadataConnectorGeneric">
        <globalRef requiredType="http://www.mulesoft.org/schema/mule/metadata/config" description="Specify which configuration to use for this invocation." caption="Config Reference" name="config-ref"></globalRef>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector extends="http://www.mulesoft.org/schema/mule/metadata/abstractMetadataConnectorGeneric" abstract="true" image="icons/large/metadata-connector-48x32.png" localId="processor" icon="icons/small/metadata-connector-24x16.png" caption="Processor">
    <attribute-category description="General." caption="General">
      <group caption="Advanced" id="advanced">
        <string required="true" description="Parameter in Advanced input group." caption="Advanced1" name="advanced1"></string>
        <string required="true" description="Parameter in Advanced input group." caption="Advanced2" name="advanced2"></string>
      </group>
      <group caption="General" id="general">
        <string required="true" description="Parameter in General input group." caption="General" name="general"></string>
        <string required="true" description="Parameter with no input group." caption="No Input Group Explicit" name="noInputGroupExplicit"></string>
      </group>
    </attribute-category>
  </cloud-connector>
  <endpoint supportsOutbound="false" supportsInbound="true" image="icons/small/metadata-connector-24x16.png" localId="source" icon="icons/small/metadata-connector-24x16.png" description="A source method with Studio metadata." caption="Source">
    <attribute-category description="General." caption="General">
      <group caption="Advanced" id="advanced">
        <string required="true" description="Parameter in Advanced input group." caption="Advanced1" name="advanced1"></string>
        <string required="true" description="Parameter in Advanced input group." caption="Advanced2" name="advanced2"></string>
      </group>
      <group caption="General" id="general">
        <string required="true" description="Parameter in General input group." caption="General" name="general"></string>
        <string required="true" description="Parameter with no input group." caption="No Input Group Explicit" name="noInputGroupExplicit"></string>
      </group>
    </attribute-category>
  </endpoint>
  <global-endpoint extends="http://www.mulesoft.org/schema/mule/metadata/source" image="icons/large/metadata-connector-48x32.png" localId="source" icon="icons/small/metadata-connector-24x16.png" description="A source method with Studio metadata." caption="source">
    <attribute-category description="General." caption="General">
      <group caption="Generic" id="abstractEndpointGeneric">
        <name description="Endpoint name." caption="Name" xsdType="substitutableClass" name="name"></name>
      </group>
    </attribute-category>
  </global-endpoint>
</namespace>