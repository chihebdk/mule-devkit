    <nested image="icons/large/mymodule-connector-48x32.png" localId="string-object-map"
            icon="icons/small/mymodule-connector-24x16.png" description="String object map."
            caption="String Object Map">
        <string description="Key." caption="Key" name="key"></string>
        <text isToElement="true" description="Value." caption="Value" name="value"></text>
    </nested>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation6"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation6">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation6 method description." id="general">
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
                <childElement allowMultiple="false" inplace="true" description="Represents the stringStringMap."
                              caption="String String Map"
                              name="http://www.mulesoft.org/schema/mule/mymodule/string-string-map"></childElement>
                <childElement allowMultiple="false" inplace="true" description="Represents the stringObjectMap."
                              caption="String Object Map"
                              name="http://www.mulesoft.org/schema/mule/mymodule/string-object-map"></childElement>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation7"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation7">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation7 method description." id="general">
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password" name="password"></string>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation8"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation8">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation8 method description." id="general">
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password" name="password"></string>
                <enum allowsCustom="true" required="true" supportsExpressions="true"
                      description="Represents the siNoEnum." caption="Si No Enum" name="siNoEnum">
                    <option value="SI" caption="Si"></option>
                    <option value="NO" caption="No"></option>
                </enum>
                <enum allowsCustom="true" required="false" supportsExpressions="true"
                      description="Represents the siNoEnumOptional." caption="Si No Enum Optional"
                      name="siNoEnumOptional">
                    <option value="SI" caption="Si"></option>
                    <option value="NO" caption="No"></option>
                </enum>
                <enum allowsCustom="true" defaultValue="NO" required="false" supportsExpressions="true"
                      description="Represents the siNoEnumOptionalDefault." caption="Si No Enum Optional Default"
                      name="siNoEnumOptionalDefault">
                    <option value="SI" caption="Si"></option>
                    <option value="NO" caption="No"></option>
                </enum>
            </group>
        </attribute-category>
