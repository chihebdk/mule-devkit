<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" prefix="mymodule"
           url="http://www.mulesoft.org/schema/mule/mymodule">
    <global-cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                            image="icons/large/mymodule-connector-48x32.png" localId="config"
                            icon="icons/small/mymodule-connector-24x16.png"
                            description="Global Mymodule configuration information." caption="Mymodule">
        <attribute-category description="Mymodule configuration properties." caption="Mymodule">
            <group caption="Generic" id="mymoduleGenericProperties">
                <name required="false"
                      description="Give a name to this configuration so it can be later referenced by config-ref."
                      caption="Name" name="name"></name>
                <string required="true" description="Configurable String." caption="Configurable String"
                        name="configurableString"></string>
                <string required="false" description="Configurable optional String."
                        caption="Optional Configurable String" name="optionalConfigurableString"></string>
                <string defaultValue="a default" required="false"
                        description="Configurable optional String with default value."
                        caption="Optional With Default Configurable String"
                        name="optionalWithDefaultConfigurableString"></string>
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
            </group>
        </attribute-category>
    </global-cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     image="icons/large/mymodule-connector-48x32.png" localId="mymodule-connector"
                     icon="icons/small/mymodule-connector-24x16.png" description="Mymodule Integration."
                     caption="Mymodule" aliasId="org.mule.tooling.ui.modules.core.pattern.mymodule">
        <attribute-category description="General properties." caption="General">
            <group caption="Generic" id="mymoduleConnectorGeneric">
                <modeSwitch description="Operation." caption="Operation" name="Mymodule operations to execute">
                    <mode modeLabel="Operation1"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation1"></mode>
                    <mode modeLabel="Operation2"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation2"></mode>
                    <mode modeLabel="Operation3"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation3"></mode>
                    <mode modeLabel="Operation4"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation4"></mode>
                    <mode modeLabel="Operation5"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation5"></mode>
                    <mode modeLabel="Operation6"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation6"></mode>
                    <mode modeLabel="Operation7"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation7"></mode>
                    <mode modeLabel="Operation8"
                          modeId="http://www.mulesoft.org/schema/mule/mymodule/operation8"></mode>
                </modeSwitch>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector abstract="true" image="icons/large/mymodule-connector-48x32.png"
                     localId="abstractMymoduleConnectorGeneric" icon="icons/small/mymodule-connector-24x16.png"
                     description="Interact with Mymodule." caption="Abstractmymoduleconnectorgeneric">
        <attribute-category description="General properties." caption="General">
            <group caption="Generic" id="abstractMymoduleConnectorGeneric">
                <globalRef requiredType="http://www.mulesoft.org/schema/mule/mymodule/config"
                           description="Specify which configuration to use for this invocation."
                           caption="Config Reference" name="config-ref"></globalRef>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation1"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation1">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation1 method description." id="general">
                <string required="true" description="StringParameter description." caption="String Parameter"
                        name="stringParameter"></string>
                <integer step="1" min="0" required="true" description="IntParameter description."
                         caption="Int Parameter" name="intParameter"></integer>
                <boolean required="true" description="BooleanParameter description." caption="Boolean Parameter"
                         name="booleanParameter"></boolean>
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation2"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation2">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation2 method description." id="general">
                <string required="false" description="StringParameter description." caption="String Parameter"
                        name="stringParameter"></string>
                <integer step="1" min="0" required="false" description="IntParameter description."
                         caption="Int Parameter" name="intParameter"></integer>
                <boolean required="false" description="BooleanParameter description." caption="Boolean Parameter"
                         name="booleanParameter"></boolean>
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation3"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation3">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation3 method description." id="general">
                <string defaultValue="fede" required="false" description="StringParameter description."
                        caption="String Parameter" name="stringParameter"></string>
                <integer step="1" min="0" defaultValue="10" required="false" description="IntParameter description."
                         caption="Int Parameter" name="intParameter"></integer>
                <boolean defaultValue="true" required="false" description="BooleanParameter description."
                         caption="Boolean Parameter" name="booleanParameter"></boolean>
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
            </group>
        </attribute-category>
    </cloud-connector>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation4"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation4">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation4 method description." id="general">
                <string required="true" description="StringParameter description." caption="String Parameter"
                        name="stringParameter"></string>
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
            </group>
        </attribute-category>
    </cloud-connector>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="integers"
            icon="icons/small/mymodule-connector-24x16.png" description="Integers." caption="Integers">
        <childElement allowMultiple="true" description="Integers." caption="Integers"
                      name="http://www.mulesoft.org/schema/mule/mymodule/integer"></childElement>
    </nested>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="integer"
            icon="icons/small/mymodule-connector-24x16.png" description="Integers." caption="Integers">
        <integer step="1" min="0" description="Represents the integers." caption="Integers"
                 name="integer"></integer>
    </nested>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="strings"
            icon="icons/small/mymodule-connector-24x16.png" description="Strings." caption="Strings">
        <childElement allowMultiple="true" description="Strings." caption="Strings"
                      name="http://www.mulesoft.org/schema/mule/mymodule/string"></childElement>
    </nested>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="string"
            icon="icons/small/mymodule-connector-24x16.png" description="Strings." caption="Strings">
        <string description="Represents the strings." caption="Strings" name="string"></string>
    </nested>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="booleans"
            icon="icons/small/mymodule-connector-24x16.png" description="Booleans." caption="Booleans">
        <childElement allowMultiple="true" description="Booleans." caption="Booleans"
                      name="http://www.mulesoft.org/schema/mule/mymodule/boolean"></childElement>
    </nested>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="boolean"
            icon="icons/small/mymodule-connector-24x16.png" description="Booleans." caption="Booleans">
        <boolean description="Represents the booleans." caption="Booleans" name="boolean"></boolean>
    </nested>
    <cloud-connector extends="http://www.mulesoft.org/schema/mule/mymodule/abstractMymoduleConnectorGeneric"
                     abstract="true" image="icons/large/mymodule-connector-48x32.png" localId="operation5"
                     icon="icons/small/mymodule-connector-24x16.png" caption="Operation5">
        <attribute-category description="General properties." caption="General">
            <group caption="General" description="Operation5 method description." id="general">
                <string required="false" description="The user name to use." caption="User" name="user"></string>
                <string required="false" description="The password to use." caption="Password"
                        name="password"></string>
                <childElement allowMultiple="false" inplace="true" description="Represents the integers."
                              caption="Integers"
                              name="http://www.mulesoft.org/schema/mule/mymodule/integers"></childElement>
                <childElement allowMultiple="false" inplace="true" description="Represents the strings."
                              caption="Strings"
                              name="http://www.mulesoft.org/schema/mule/mymodule/strings"></childElement>
                <childElement allowMultiple="false" inplace="true" description="Represents the booleans."
                              caption="Booleans"
                              name="http://www.mulesoft.org/schema/mule/mymodule/booleans"></childElement>
            </group>
        </attribute-category>
    </cloud-connector>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="string-string-map"
            icon="icons/small/mymodule-connector-24x16.png" description="String string map."
            caption="String String Map">
        <childElement allowMultiple="true" description="String string map." caption="String String Map"
                      name="http://www.mulesoft.org/schema/mule/mymodule/string-string-map"></childElement>
    </nested>
    <nested image="icons/large/mymodule-connector-48x32.png" localId="string-string-map"
            icon="icons/small/mymodule-connector-24x16.png" description="String string map."
