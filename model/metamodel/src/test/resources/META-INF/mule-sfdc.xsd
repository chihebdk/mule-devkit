                    <xs:element name="ids" maxOccurs="1" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Array of one or more IDs associated with the objects to delete.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="id" maxOccurs="unbounded" minOccurs="0">
